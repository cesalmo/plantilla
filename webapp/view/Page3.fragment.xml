    xmlns:core="sap.ui.core" 
    xmlns:f="sap.ui.layout.form">
    
        <Page 
            id="p3" 
            title="Lista empleados" 
            backgroundDesign="List">
            <content>
                <Table 
                    id="p3Tabla1"
                    items="{/data}"
                    >
                    <columns>
                        <Column >
                            <Text text="Id."/>
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem >
                            <cells>
                                <ObjectIdentifier 
                                    title="{id}"
                                    text="{employee_name}"
                                ></ObjectIdentifier>
                            </cells>                        
                        </ColumnListItem>
                    </items>
                </Table>
            </content>
            <footer>
                <Bar> 
                    <contentLeft>
                        <Button text="AtrÃ¡s" press="page3OnNavPage0" icon="sap-icon://nav-back"  />
                    </contentLeft>
                </Bar>
            </footer>
        </Page>
</core:FragmentDefinition>
