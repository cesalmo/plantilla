{
  "_version": "1.1.0",
  "sap.app": {
    "title": "{{title}}",
    "applicationVersion": {
      "version": "1.1.0"
    },
    "id": "cam.its",
    "type": "application"

  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "phone": true,
      "tablet": true,
      "desktop": true
    },
    "supportedThemes": [
      "sap_belize"
    ]
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
        
      }
    },
    "rootView": {
      "viewName": "cam.its.view.App",
      "type": "XML",
      "async": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "cam.its.i18n.i18n",
          "supportedLocales": ["es"],
          "fallbackLocale": ""
        }
      },
      "page1": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/page1.json",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      },
      "employees": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/employees.json",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "routing": {
      "config": {},
      "routes": [],
      "targets": {}
    },
    "resources": {
      "css": [
        {
          "uri": "css/styles.css"
        }
      ]
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "resourceRoots": {}
  }
}